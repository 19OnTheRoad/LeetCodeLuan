class Solution:
    def shipWithinDays(self, nums: List[int], days: int) -> int:
        i, j = max(nums), sum(nums)
        while i<j:
            mid = (i+j)//2
            tempSum, count = 0, 1
            for num in nums:
                if tempSum + num > mid:
                    tempSum = num
                    count += 1
                else: 
                    tempSum += num
            if count<=days: j = mid
            else: i = mid + 1
        return i
